<% include ./partials/header %>


<div class="container-fluid">

	<div class="jumbotron">
		<h1 class="blackopsone">SHOW PAGE</h1>
	</div>

	<div id="main">

		<section id="trash-item-details">

			<div class="trashytrash">
				<h3><%= trash.name %></h3>
				<p><%= trash.category %></p>
				<p><%= trash.color %></p>
				<p>Notes: <%= trash.notes %></p>
				<a href="/trash/<%= trash._id %>/edit">edit</a>
				<form id="delete" action="/trash/<%= trash._id %>?_method=DELETE" method="POST">
					<button>Delete</button>
				</form>
			</div>

		</section>

	</div>
</div>


<% include ./partials/footer %>